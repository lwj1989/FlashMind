{
  "name": "flashmind-frontend",
  "version": "1.0.0",
  "description": "FlashMind 前端应用",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,vue,ts}\"",
    "format:check": "prettier --check \"src/**/*.{js,vue,ts}\"",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "axios": "^1.5.0",
    "element-plus": "^2.3.9",
    "marked": "^5.1.2",
    "highlight.js": "^11.8.0",
    "@element-plus/icons-vue": "^2.1.0",
    "js-cookie": "^3.0.5",
    "dayjs": "^1.11.9"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "vite": "^4.4.5",
    "vitest": "^0.34.1",
    "@vue/test-utils": "^2.4.1",
    "happy-dom": "^10.9.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "@vue/eslint-config-standard": "^8.0.1",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.27",
    "@types/js-cookie": "^3.0.3",
    "vue-tsc": "^1.8.8",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}